{
  "masterPicture": "assets/images/profile.jpg",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "noChange",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      }
    },
    "desktopBrowser": {},
    "windows": {
      "pictureAspect": "noChange",
      "backgroundColor": "#da532c",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      }
    },
    "androidChrome": {
      "pictureAspect": "backgroundAndMargin",
      "margin": "6%",
      "backgroundColor": "#ffffff",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "mkw.github.io",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": true,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "blackAndWhite",
      "threshold": 50,
      "themeColor": "#25396b"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false
  }
}
